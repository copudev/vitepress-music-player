{
  "name": "@copudev/vitepress-music-player",
  "version": "1.0.0",
  "description": "ðŸŽµ A modern, responsive music player component for VitePress with FLAC support",
  "keywords": [
    "vitepress",
    "vue3",
    "music-player",
    "audio",
    "flac",
    "typescript",
    "component"
  ],
  "main": "dist/index.umd.js",
  "module": "dist/index.es.js",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/index.es.js",
      "require": "./dist/index.umd.js",
      "types": "./dist/index.d.ts"
    },
    "./style.css": "./dist/style.css"
  },
  "files": [
    "dist",
    "src",
    "README.md",
    "LICENSE"
  ],
  "scripts": {
    "dev": "vite",
    "build": "vite build && tsc --emitDeclarationOnly --outDir dist",
    "build:docs": "vitepress build docs",
    "preview": "vite preview",
    "serve:docs": "vitepress serve docs",
    "test": "vitest",
    "lint": "eslint src --ext ts,vue --report-unused-disable-directives --max-warnings 0",
    "type-check": "tsc --noEmit",
    "prepublishOnly": "npm run build"
  },
  "peerDependencies": {
    "vue": "^3.2.0",
    "vitepress": "^1.0.0"
  },
  "devDependencies": {
    "@types/node": "^20.5.2",
